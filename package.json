{
  "name": "@ObserveRTC/observer-lib",
  "version": "2103-27",
  "description": "Extractor Development Toolkits for WebRTC Samples",
  "main": "dist/latest/observer.js",
  "types": "dist/latest/observer.d.ts",
  "config": {
    "development": {
      "_workerLoadEndpoint": "http://localhost:9090/js/dist",
      "workerLoadEndpoint": "https://observertc.github.io/observer-js/dist",
      "collectorLibraryFileName": "observer.js",
      "workerLibraryFileName": "observer.worker.js"
    },
    "production": {
      "_workerLoadEndpoint": "http://localhost:9090/js/dist",
      "workerLoadEndpoint": "https://observertc.github.io/observer-js/dist",
      "collectorLibraryFileName": "observer.min.js",
      "workerLibraryFileName": "observer.worker.min.js"
    },
    "libraryFileNameDefinition": "observer.d.ts",
    "collectorRootFilePath": "./src/observer.collector/__package__/index.ts",
    "workerRootFilePath": "./src/observer.processor/__package__/index.ts",
    "collectorLibraryOutput": {
      "name": "ObserverRTC",
      "exports": "named"
    },
    "workerLibraryOutput": {
      "name": "ObserverWorkerRTC",
      "exports": "named"
    }
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.0.4",
    "bowser": "^2.11.0",
    "lodash": "^4.17.20",
    "loglevel": "^1.6.8",
    "reconnecting-websocket": "^4.4.0",
    "tslib": "^2.0.3",
    "uuid": "^8.1.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "@surma/rollup-plugin-off-main-thread": "^1.4.2",
    "@types/jasmine": "^3.5.14",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.167",
    "@types/loglevel": "^1.6.3",
    "@types/ua-parser-js": "^0.7.33",
    "@types/uuid": "^8.3.0",
    "@types/webrtc": "0.0.26",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.16.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-glob-import": "^0.4.5",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "terser-webpack-plugin": "^4.2.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "ua-parser-js": "^0.7.22",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "build": "rollup --config rollup.api.config.js",
    "build-dev": "rollup --config rollup.api.config.js",
    "build-latest": "rollup --config rollup.api.config.js",
    "publish": "npm run build && npm run build-dev && npm run build-latest",
    "lint": "eslint --ext .ts .",
    "lint:fix": "yarn lint --fix",
    "test": "jest"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "url": "git@github.com:ObserveRTC/observer-js.git"
  }
}
